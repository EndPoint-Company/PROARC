<Page
        x:Class="PROARC.src.Views.RegistrarProcesso01Page"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" xmlns:sys="using:System"
        mc:Ignorable="d"
        Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

    <Grid Background="#FAFAFA">
        <!-- Estilos Comuns -->
        <Grid.Resources>
            <Style TargetType="TextBox">
                <Setter Property="Margin" Value="5"/>
                <Setter Property="Padding" Value="10"/>
                <Setter Property="CornerRadius" Value="5"/>
            </Style>

            <ThemeShadow x:Name="SharedShadow" />
        </Grid.Resources>
        <ScrollViewer>
            <StackPanel Padding="20" Spacing="32" Margin="0,0,0, 40">
                <!-- Título -->
                <TextBlock Text="CADASTRAR RECLAMAÇÃO" 
                               FontSize="32" 
                               FontWeight="Bold" 
                               Margin="160,40, 160, 0" />

                <StackPanel Orientation="Horizontal" HorizontalAlignment="Left" Padding="10" Margin="160, 0" Spacing="10">
                    <Button x:Name="btnProcessoNovo" 
                            Content="Reclamação Nova" 
                            Padding="20,10" 
                            Background="#003366" 
                            Foreground="White" 
                            Click="ProcessoNovo_Click" />
                    <Button x:Name="btnProcessoAntigo" 
                            Content="Reclamação Antiga" 
                            Padding="20,10" 
                            Background="White" 
                            BorderBrush="#005BC1" 
                            Foreground="#003366"     
                            Click="ProcessoAntigo_Click" />
                </StackPanel>


                <StackPanel Orientation="Horizontal" Margin="160,0">
                    <StackPanel Orientation="Vertical" Spacing="32">

                        <!-- Seção Processo / data -->
                        <StackPanel x:Name="MainStackPanel" Orientation="Horizontal" Opacity="1">
                            <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />
                            <StackPanel Orientation="Horizontal">
                                <StackPanel Padding="40" Background="#FFF" Spacing="10" CornerRadius="0, 10, 10, 0" x:Name="NProcessoSection" Shadow="{StaticResource SharedShadow}" Width="796" Height="186" Orientation="Horizontal">
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock x:Name="TextBlockNProcesso" Text="N° Reclamação" FontSize="18" FontWeight="Bold" />
                                        <TextBox x:Name="inputNProcesso" PlaceholderText="Insira o n° do processo" Width="200" Text="{Binding NumeroProcesso}" IsReadOnly="False" />
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical" Margin="10, 0, 0, 0">
                                        <TextBlock x:Name="TextBlockAnoProcesso" Text="Ano" FontSize="18" FontWeight="Bold" />
                                        <TextBox x:Name="inputAnoProcesso" PlaceholderText="Insira o ano do processo" Width="200" Text="{Binding AnoProcesso}" IsReadOnly="False" />
                                    </StackPanel>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>

                        <StackPanel Orientation="Horizontal" Spacing="32">

                            <!-- Seção Motivo -->
                            <StackPanel Orientation="Horizontal">
                                <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />
                                <StackPanel Padding="40" Background="#FFF" Spacing="10" Height="186" CornerRadius="0, 10, 10, 0" x:Name="MotivoSection" Shadow="{StaticResource SharedShadow}" Width="376">
                                    <Grid>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="*" />
                                            <ColumnDefinition Width="Auto" />
                                        </Grid.ColumnDefinitions>
                                        <TextBlock x:Name="TextBlockMotivo" Text="Motivo" FontSize="18" FontWeight="Bold" VerticalAlignment="Center" Grid.Column="0" />
                                    </Grid>
                                    <StackPanel Orientation="Horizontal">
                                        <ComboBox x:Name="cbMotivo" PlaceholderText="Selecione o motivo" />
                                        <Button Content="Novo Motivo" Grid.Column="1" Click="OnNovoMotivoClick" Background="#003366" Foreground="White" Margin="10,0,0,0"/>
                                    </StackPanel>
                                </StackPanel>
                            </StackPanel>

                            <!-- Seção Audiência -->
                            <StackPanel Orientation="Horizontal">
                                <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />
                                <StackPanel Padding="40" Height="186" Background="#FFF" Spacing="10" CornerRadius="0, 10, 10, 0" Width="376" x:Name="AudienciaSection" Shadow="{StaticResource SharedShadow}">
                                    <TextBlock Text="Audiência" x:Name="TextBlockCalendario" FontSize="18" FontWeight="Bold" />

                                    <StackPanel Orientation="Horizontal">
                                        <!-- Seletor de Data -->
                                        <CalendarDatePicker x:Name="calendario" PlaceholderText="Escolha a data" />

                                        <!-- Botão que abre o TimePickerFlyout -->
                                        <Button x:Name="btnSelecionarHora" Content="Escolher Horário" Click="SelecionarHora_Click" Margin="10,0,0,0"/>
                                    </StackPanel>

                                </StackPanel>
                            </StackPanel>
                        </StackPanel>

                    </StackPanel>

                    <!-- Seção Status -->
                    <StackPanel Orientation="Horizontal" Margin="32,0,0,0">
                        <!-- Barra lateral azul -->
                        <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10"/>

                        <!-- Conteúdo da seção Status -->
                        <StackPanel Padding="40, 30" Background="#FFF" Spacing="20" CornerRadius="0, 10, 10, 0" x:Name="StatusSection" Shadow="{StaticResource SharedShadow}" Width="638" >
                            <!-- Título -->
                            <TextBlock x:Name="TextBlockStatus" Text="Situação" FontSize="18" FontWeight="Bold" />

                            <!-- Status: Em tramitação -->
                            <StackPanel>
                                <TextBlock x:Name="TextBlockTramitacao" Text="Em tramitação" FontSize="14" FontWeight="SemiBold" Margin="0, 5"/>
                                <StackPanel>
                                    <RadioButton x:Name="radio_agFazerNotificacao" Content="Aguardando fazer notificação" GroupName="StatusGroup"/>
                                    <RadioButton x:Name="radio_agRealizacaoAudiencia" Content="Aguardando realização da audiência" GroupName="StatusGroup"/>
                                    <RadioButton x:Name="radio_agResposta" Content="Aguardando resposta do reclamado" GroupName="StatusGroup"/>
                                    <RadioButton x:Name="radio_agEnvioNotificacao" Content="Aguardando envio de notificação" GroupName="StatusGroup"/>
                                    <RadioButton x:Name="radio_agDocumentacao" Content="Aguardando documentação das partes e assinatura" GroupName="StatusGroup"/>
                                </StackPanel>
                            </StackPanel>

                            <!-- Status: Arquivado -->
                            <StackPanel>
                                <TextBlock x:Name="TextBlockArquivado" Text="Arquivado" FontSize="14" FontWeight="SemiBold" Margin="0, 5"/>
                                <StackPanel>
                                    <RadioButton x:Name="radio_atendido" Content="Atendido" GroupName="StatusGroup"/>
                                    <RadioButton x:Name="radio_naoAtendido" Content="Não Atendido" GroupName="StatusGroup"/>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>

                <Grid Margin="160, 0">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="Auto"/>
                        <ColumnDefinition Width="32"/>
                        <ColumnDefinition Width="Auto"/>
                    </Grid.ColumnDefinitions>

                    <!-- Seção Reclamante -->
                    <Grid Grid.Column="0">
                        <Border Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" HorizontalAlignment="Left"/>

                        <Border x:Name="ReclamanteSection" CornerRadius="0, 10, 10, 0" Shadow="{StaticResource SharedShadow}"
                    Padding="40" Background="#FFF" Width="1000" Margin="10, 0, 0, 0">
                            <StackPanel Spacing="10">
                                <TextBlock x:Name="TextBlockReclamante" Text="Reclamante" FontSize="18" FontWeight="Bold" />
                                <StackPanel Orientation="Horizontal" Spacing="20">
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock x:Name="TextBlockNome" Text="Nome *" FontSize="14"/>
                                        <TextBox x:Name="inputNome" PlaceholderText="Insira o nome completo" Width="300"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock x:Name="TextBlockCpfReclamante" Text="CPF *" FontSize="14"/>
                                        <TextBox x:Name="inputCpfReclamante" TextChanged="OnCpfTextChanged" MaxLength="14" PlaceholderText="Insira o CPF" Width="188"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock Text="RG" FontSize="14"/>
                                        <TextBox x:Name="inputRgReclamante" PlaceholderText="Insira o RG" Width="188"/>
                                    </StackPanel>
                                </StackPanel>
                                <StackPanel Orientation="Horizontal" Spacing="20">
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock x:Name="TextBlockEmailReclamante" Text="E-mail" FontSize="14"/>
                                        <TextBox x:Name="inputEmailReclamante" PlaceholderText="Insira o e-mail completo" Width="300"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock x:Name="TextBlockCpfNumeroReclamante" Text="Telefone Celular" FontSize="14"/>
                                        <TextBox x:Name="inputNumeroReclamante" PlaceholderText="Insira o número celular" Width="188"/>
                                    </StackPanel>
                                </StackPanel>
                                <CheckBox x:Name="ProcuradorCheckBox" Content="Tem procurador" Margin="10" Checked="ProcuradorCheckBox_Checked" Unchecked="ProcuradorCheckBox_Unchecked" />
                            </StackPanel>
                        </Border>
                    </Grid>

                    <!-- Seção Conciliador -->
                    <Grid Grid.Column="2">
                        <Border Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" HorizontalAlignment="Left"/>
                        <Border x:Name="ConciliadorSection" CornerRadius="0, 10, 10, 0" Shadow="{StaticResource SharedShadow}"
                    Padding="40" Background="#FFF" Width="434" Margin="10, 0, 0, 0">
                            <StackPanel Spacing="10">
                                <TextBlock x:Name="TextBlockConciliador" Text="Conciliador(a)" FontSize="18" FontWeight="Bold" />
                                <StackPanel Orientation="Horizontal" Spacing="20">
                                    <StackPanel Orientation="Vertical">
                                        <TextBlock x:Name="TextBlockNomeConciliador" Text="Nome *" FontSize="14"/>
                                        <TextBox x:Name="inputNomeConciliador" PlaceholderText="Insira o nome do(a) conciliador(a)" Width="300"/>
                                    </StackPanel>
                                </StackPanel>
                            </StackPanel>
                        </Border>
                    </Grid>
                </Grid>

                <!-- Contêiner principal para o StackPanel Procurador -->
                <StackPanel Orientation="Horizontal" Margin="160, 0" x:Name="ProcuradorSection1" Visibility="Collapsed">
                    <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />

                    <!-- Seção Procurador -->
                    <StackPanel Padding="40" Background="#FFF" Spacing="10" x:Name="ProcuradorSection2" Shadow="{StaticResource SharedShadow}" Width="1478" CornerRadius="0, 10, 10, 0">
                        <TextBlock x:Name="TextBlockProcurador" Text="Procurador" FontSize="18" FontWeight="Bold" />

                        <!-- Primeira Linha -->
                        <StackPanel Orientation="Horizontal" Spacing="20">
                            <StackPanel Orientation="Vertical">
                                <TextBlock Text="Nome *" FontSize="14"/>
                                <TextBox x:Name="inputNomeProcurador" PlaceholderText="Insira o nome completo" Width="300" />
                            </StackPanel>
                            <StackPanel Orientation="Vertical">
                                <TextBlock Text="CPF *" FontSize="14"/>
                                <TextBox x:Name="inputCpfProcurador" PlaceholderText="Insira o CPF" Width="188" MaxLength="14" TextChanged="OnCpfTextChanged" />
                            </StackPanel>
                            <StackPanel Orientation="Vertical">
                                <TextBlock Text="RG" FontSize="14"/>
                                <TextBox x:Name="inputRgProcurador" PlaceholderText="Insira o RG" Width="188" />
                            </StackPanel>
                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel Orientation="Vertical">
                                    <TextBlock x:Name="TextBlockEmailProcurado" Text="E-mail" FontSize="14"/>
                                    <TextBox x:Name="inputEmailProcurador" PlaceholderText="Insira o e-mail completo" Width="300"/>
                                </StackPanel>
                                <StackPanel Orientation="Vertical">
                                    <TextBlock x:Name="TextBlockCpfNumeroProcurado" Text="Telefone Celular" FontSize="14"/>
                                    <TextBox x:Name="inputNumeroProcurador" MaxLength="14" PlaceholderText="Insira o número celular" Width="188"/>
                                </StackPanel>
                            </StackPanel>
                        </StackPanel>
                    </StackPanel>
                </StackPanel>

                <!-- Contêiner principal para as Stacks Reclamados-->
                <StackPanel>
                    <!-- Reclamado 01-->
                    <StackPanel x:Name="Reclamado01" Orientation="Horizontal" Margin="160, 0">
                        <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />

                        <StackPanel Padding="40" Background="#FFF" Spacing="10" x:Name="CadastrarEmpresaSectionReclamado01" Shadow="{StaticResource SharedShadow}" Width="1478" CornerRadius="0, 10, 10, 0">
                            <StackPanel Orientation="Horizontal">
                                <TextBlock x:Name="textBlockReclamado01" Text="1° Reclamado" FontSize="20" FontWeight="Bold" />
                                <ComboBox x:Name="comboBoxQuantidadeReclamado" PlaceholderText="Quantidade de Reclamados" SelectionChanged="OnQuantidadeReclamados" Margin="10,0,0,0" Padding="10" Width="200">
                                    <ComboBoxItem Content="01"/>
                                    <ComboBoxItem Content="02"/>
                                    <ComboBoxItem Content="03"/>
                                    <ComboBoxItem Content="04"/>
                                    <ComboBoxItem Content="05"/>
                                    <ComboBoxItem Content="06"/>
                                    <ComboBoxItem Content="07"/>
                                    <ComboBoxItem Content="08"/>
                                    <ComboBoxItem Content="09"/>
                                    <ComboBoxItem Content="10"/>
                                    <ComboBoxItem Content="11"/>
                                    <ComboBoxItem Content="12"/>
                                    <ComboBoxItem Content="13"/>
                                    <ComboBoxItem Content="14"/>
                                    <ComboBoxItem Content="15"/>
                                    <ComboBoxItem Content="16"/>
                                </ComboBox>
                            </StackPanel>

                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel>
                                    <TextBlock x:Name="TextBlockNomeReclamado01" Text="Instituição *" FontSize="14"/>
                                    <TextBox x:Name="inputNomeReclamado01" PlaceholderText="Insira o nome da Instituição" Width="250" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="Tipo de Documento" FontSize="14" Margin="0, 0, 0, 5"/>
                                    <ComboBox x:Name="comboBoxTipoDocumentoReclamado01" PlaceholderText="Selecione CNPJ ou CPF" SelectionChanged="OnTipoDocumentoChangedReclamado01" Padding="11" Width="200">
                                        <ComboBoxItem Content="CPF"/>
                                        <ComboBoxItem Content="CNPJ"/>
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="CNPJ/CPF" FontSize="14"/>
                                    <TextBox x:Name="inputCnpjCpfReclamado01" MaxLength="14" TextChanged="OnCpfCnpjTextChangedReclamado01" 
                PlaceholderText="Insira CNPJ ou CPF" Width="200" IsEnabled="False" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="E-mail" FontSize="14"/>
                                    <TextBox x:Name="inputEmailReclamado01" PlaceholderText="Insira o E-mail" Width="200" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical">
                                    <TextBlock Text="Telefone" FontSize="14"/>
                                    <TextBox x:Name="inputTelefoneReclamado01" PlaceholderText="Insira o Telefone" Width="240" />
                                </StackPanel>
                            </StackPanel>

                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel>
                                    <TextBlock x:Name="TextBlockRuaReclamado01" Text="Rua *" FontSize="14"/>
                                    <TextBox x:Name="inputRuaReclamado01" PlaceholderText="Insira a rua" Width="250" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockNumeroReclamado01" Text="Número *" FontSize="14"/>
                                    <TextBox x:Name="inputNumeroReclamado01" PlaceholderText="Insira o número" Width="120" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockBairroReclamado01" Text="Bairro *" FontSize="14"/>
                                    <TextBox x:Name="inputBairroReclamado01" PlaceholderText="Insira o bairro" Width="270" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockCidadeReclamado01" Text="Cidade *" FontSize="14"/>
                                    <TextBox x:Name="inputCidadeReclamado01" PlaceholderText="Insira a cidade" Width="200" />
                                </StackPanel>


                                <StackPanel>
                                    <TextBlock x:Name="TextBlockUfReclamado01" Text="UF *" FontSize="14" Margin="0, 0, 0, 5"/>
                                    <ComboBox x:Name="comboBoxUfReclamado01" PlaceholderText="Selecione a UF" Padding="11" Width="88"/>
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockCepReclamado01" Text="CEP *" FontSize="14" />
                                    <TextBox x:Name="inputCepReclamado01" PlaceholderText="Insira o CEP" MaxLength="9" Width="132" />
                                </StackPanel>
                            </StackPanel>

                        </StackPanel>
                    </StackPanel>

                    <!-- Reclamado 02-->
                    <StackPanel x:Name="Reclamado02" Orientation="Horizontal" Margin="160, 32, 0, 0">
                        <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />

                        <StackPanel Padding="40" Background="#FFF" Spacing="10" x:Name="CadastrarEmpresaSectionReclamado02" Shadow="{StaticResource SharedShadow}" Width="1478" CornerRadius="0, 10, 10, 0">
                            <TextBlock x:Name="textBlockReclamado02" Text="2° Reclamado" FontSize="20" FontWeight="Bold" />

                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel>
                                    <TextBlock x:Name="TextBlockNomeReclamado02" Text="Instituição *" FontSize="14"/>
                                    <TextBox x:Name="inputNomeReclamado02" PlaceholderText="Insira o nome da Instituição" Width="250" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="Tipo de Documento" FontSize="14" Margin="0, 0, 0, 5"/>
                                    <ComboBox x:Name="comboBoxTipoDocumentoReclamado02" PlaceholderText="Selecione CNPJ ou CPF" SelectionChanged="OnTipoDocumentoChangedReclamado02" Padding="10" Width="200">
                                        <ComboBoxItem Content="CPF"/>
                                        <ComboBoxItem Content="CNPJ"/>
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="CNPJ/CPF" FontSize="14"/>
                                    <TextBox x:Name="inputCnpjCpfReclamado02" MaxLength="14" TextChanged="OnCpfCnpjTextChangedReclamado02" PlaceholderText="Insira CNPJ ou CPF" Width="200" IsEnabled="False" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="E-mail" FontSize="14"/>
                                    <TextBox x:Name="inputEmailReclamado02" PlaceholderText="Insira o E-mail" Width="200" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical">
                                    <TextBlock Text="Telefone" FontSize="14"/>
                                    <TextBox x:Name="inputTelefoneReclamado02" PlaceholderText="Insira o Telefone" Width="240" />
                                </StackPanel>
                            </StackPanel>

                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel>
                                    <TextBlock x:Name="TextBlockRuaReclamado02" Text="Rua *" FontSize="14"/>
                                    <TextBox x:Name="inputRuaReclamado02" PlaceholderText="Insira a rua" Width="250" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockNumeroReclamado02" Text="Número *" FontSize="14"/>
                                    <TextBox x:Name="inputNumeroReclamado02" PlaceholderText="Insira o número" Width="120" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockBairroReclamado02" Text="Bairro *" FontSize="14"/>
                                    <TextBox x:Name="inputBairroReclamado02" PlaceholderText="Insira o bairro" Width="270" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockCidadeReclamado02" Text="Cidade *" FontSize="14"/>
                                    <TextBox x:Name="inputCidadeReclamado02" PlaceholderText="Insira a cidade" Width="200" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockUfReclamado02" Text="UF *" FontSize="14" Margin="0, 0, 0, 5"/>
                                    <ComboBox x:Name="comboBoxUfReclamado02" PlaceholderText="Selecione a UF" Padding="11" Width="88"/>
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockCepReclamado02" Text="CEP *" FontSize="14"/>
                                    <TextBox x:Name="inputCepReclamado02" PlaceholderText="Insira o CEP" MaxLength="9" Width="132"/>
                                </StackPanel>
                            </StackPanel>

                        </StackPanel>
                    </StackPanel>

                    <!-- Reclamado 03-->
                    <StackPanel x:Name="Reclamado03" Orientation="Horizontal" Margin="160, 32, 0, 0">
                        <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />

                        <StackPanel Padding="40" Background="#FFF" Spacing="10" x:Name="CadastrarEmpresaSectionReclamado03" Shadow="{StaticResource SharedShadow}" Width="1478" CornerRadius="0, 10, 10, 0">
                            <TextBlock x:Name="textBlockReclamado03" Text="3° Reclamado" FontSize="20" FontWeight="Bold" />

                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel>
                                    <TextBlock x:Name="TextBlockNomeReclamado03" Text="Instituição *" FontSize="14"/>
                                    <TextBox x:Name="inputNomeReclamado03" PlaceholderText="Insira o nome da Instituição" Width="250" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="Tipo de Documento" FontSize="14" Margin="0, 0, 0, 5"/>
                                    <ComboBox x:Name="comboBoxTipoDocumentoReclamado03" PlaceholderText="Selecione CNPJ ou CPF" SelectionChanged="OnTipoDocumentoChangedReclamado03" Padding="10" Width="200">
                                        <ComboBoxItem Content="CPF"/>
                                        <ComboBoxItem Content="CNPJ"/>
                                    </ComboBox>
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="CNPJ/CPF" FontSize="14"/>
                                    <TextBox x:Name="inputCnpjCpfReclamado03" MaxLength="14" TextChanged="OnCpfCnpjTextChangedReclamado03" PlaceholderText="Insira CNPJ ou CPF" Width="200" IsEnabled="False" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock Text="E-mail" FontSize="14"/>
                                    <TextBox x:Name="inputEmailReclamado03" PlaceholderText="Insira o E-mail" Width="200" />
                                </StackPanel>

                                <StackPanel Orientation="Vertical">
                                    <TextBlock Text="Telefone" FontSize="14"/>
                                    <TextBox x:Name="inputTelefoneReclamado03" PlaceholderText="Insira o Telefone" Width="240" />
                                </StackPanel>
                            </StackPanel>

                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel>
                                    <TextBlock x:Name="TextBlockRuaReclamado03" Text="Rua *" FontSize="14"/>
                                    <TextBox x:Name="inputRuaReclamado03" PlaceholderText="Insira a rua" Width="250" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockNumeroReclamado03" Text="Número *" FontSize="14"/>
                                    <TextBox x:Name="inputNumeroReclamado03" PlaceholderText="Insira o número" Width="120" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockBairroReclamado03" Text="Bairro *" FontSize="14"/>
                                    <TextBox x:Name="inputBairroReclamado03" PlaceholderText="Insira o bairro" Width="270" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockCidadeReclamado03" Text="Cidade *" FontSize="14"/>
                                    <TextBox x:Name="inputCidadeReclamado03" PlaceholderText="Insira a cidade" Width="200" />
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockUfReclamado03" Text="UF *" FontSize="14" Margin="0, 0, 0, 5"/>
                                    <ComboBox x:Name="comboBoxUfReclamado03" PlaceholderText="Selecione a UF" Padding="11" Width="88"/>
                                </StackPanel>

                                <StackPanel>
                                    <TextBlock x:Name="TextBlockCepReclamado03" Text="CEP *" FontSize="14"/>
                                    <TextBox x:Name="inputCepReclamado03" PlaceholderText="Insira o CEP" MaxLength="9" Width="132"/>
                                </StackPanel>
                            </StackPanel>

                        </StackPanel>
                    </StackPanel>

                    <!-- Reclamado 04-->
                    <StackPanel x:Name="Reclamado04" Orientation="Horizontal" Margin="160, 32, 0, 0">
                        <StackPanel Background="#003366" Width="10" CornerRadius="10, 0, 0, 10" />

                        <StackPanel Padding="40" Background="#FFF" Spacing="10" x:Name="CadastrarEmpresaSectionReclamado04" Shadow="{StaticResource SharedShadow}" Width="1478" CornerRadius="0, 10, 10, 0">
                            <TextBlock x:Name="textBlockReclamado04" Text="4° Reclamado" FontSize="20" FontWeight="Bold" />

                            <StackPanel Orientation="Horizontal" Spacing="20">
                                <StackPanel>
                                    <TextBlock x:Name="TextBlockNomeReclamado04" Text="Instituição *" FontSize="14"/>
                                    <TextBox x:Name="inputNomeReclamado04" PlaceholderText="Insira o nome da Instituição" Width="250" />
